function findMin(arr, num){
    let ans = -1
    let start = 0
    let end = arr.length-1

    while(start <= end){
        let mid = parseInt((end-start)/2) + start

        if(arr[mid] <= num){
            ans = arr[mid]
            start = mid+1
        } else {
            end = mid-1
        }
    }
    return ans
}

function findMax(arr, num){
    let ans = -1
    let start = 0
    let end = arr.length-1

    while(start <= end){
        let mid = parseInt((end-start)/2) + start

        if(arr[mid] >= num){
            ans = arr[mid]
            end = mid-1
        } else {
            start = mid+1
        }
    }
    return ans
}

function findMinAndMax(arr, num){
    let ans = new Array(2).fill(0)

    ans[0] = findMin(arr, num)
    ans[1] = findMax(arr, num)

    return ans
}

var preorderTraversal = function(root) {
    let ans = []
    function solve(root){
        if(!root){
            return
        }

        ans.push(root.val)
        solve(root.left)
        solve(root.right)
    }
    solve(root)
    return ans
}

var closestNodes = function(root, queries) {
    let arr = preorderTraversal(root)
    arr.sort((a,b) => a-b)

    let ans = new Array(queries.length).fill(0)
    for(let i = 0; i < queries.length; i++){
        ans[i] = findMinAndMax(arr, queries[i])
    }
    return ans
};